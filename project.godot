config/icon="res://main_icon.png"

[autoload]

Global="*res://Scripts/Global.gd"
AudioManager="*res://Scenes/AudioManager/AudioManager.tscn"
MobileAds="*res://addons/admob/src/singletons/MobileAds.gd"

[display]

window/size/width=640
window/size/height=360
window/size/test_width=740
window/size/test_height=360
window/stretch/mode="2d"
window/stretch/aspect="expand"

[editor_plugins]

enabled=PoolStringArray( "res://addons/admob/plugin.cfg" )

